{
  "name": "steadlane/silverstripe-cloudflare",
  "description": "This module aims to relieve the stress of using CloudFlare caching with any SilverStripe project. Adds extension hooks that clears CloudFlares cache for a specific page when that page is published or unpublished.",
  "type": "silverstripe-module",
  "version": "2.0",
  "keywords": [
    "cache",
    "cloudflare"
  ],
  "license": "BSD (3-Clause)",
  "authors": [
    {
      "name": "Reece Alexander",
      "email": "reece@steadlane.com.au"
    }
  ],
  "support": {
    "issues": "http://github.com/steadlane/silverstripe-cloudflare/issues"
  },
  "require": {
    "php": ">=5.5",
    "silverstripe/framework": "^4.0"
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "extra": {
        "installer-name": "cloudflare"
    },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/steadlane/silverstripe-cloudflare"
    }
  ]
}
